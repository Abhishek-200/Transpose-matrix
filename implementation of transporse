#include<stdio.h>
#include<conio.h>
int main(){
printf("ABHISHEK,0000000000\n");	
	int a[100][100],m,n;
	printf("enter no. of rows\n");
	scanf("%d",&m);
	printf("enter no. of cols\n");
	scanf("%d",&n);
	
	printf("enter elements of array\n");
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			scanf("%d",&a[i][j]);
		}
	}
	printf("transpose of array:\n");
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			printf("%d ",a[j][i]);
		}
		printf("\n");
	}
	
	return 0;
}
